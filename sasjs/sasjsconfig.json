{
  "$schema": "https://cli.sasjs.io/sasjsconfig-schema.json",
  "macroFolders": [
    "sasjs/macros"
  ],
  "serviceConfig": {
    "serviceFolders": [
      "sasjs/services/common",
      "sasjs/services/edit"
    ]
  },
  "defaultTarget": "viya",
  "targets": [
    {
      "name": "viya",
      "serverUrl": "https://sas.analytium.co.uk",
      "serverType": "SASVIYA",
      "allowInsecureRequests": false,
      "appLoc": "/Public/app/folder-navigator",
      "macroFolders": [],
      "programFolders": [],
      "buildConfig": {
        "buildOutputFileName": "buildviya.sas",
        "buildResultsFolder": "sasjsresults",
        "buildOutputFolder": "sasjsbuild",
        "initProgram": "",
        "termProgram": "",
        "macroVars": {}
      },
      "streamConfig": {
        "assetPaths": [],
        "streamWeb": true,
        "streamWebFolder": "web",
        "webSourcePath": "dist",
        "streamServiceName": "clickme"
      },
      "deployConfig": {
        "deployServicePack": true,
        "deployScripts": []
      },
      "contextName": "SAS Job Execution compute context"
    },
    {
      "name": "sas9",
      "serverType": "SAS9",
      "appLoc": "/User Folders/&sysuserid/folder-navigator",
      "deployConfig": {
        "deployScripts": [
          "sasjs/utils/sas9deploy.sh"
        ]
      },
      "buildConfig": {
        "buildOutputFileName": "buildsas9.sas"
      },
      "serverName": "SASApp",
      "repositoryName": "Foundation",
      "streamConfig": {
        "assetPaths": [
          "./assets"
        ],
        "streamWeb": true,
        "streamWebFolder": "web",
        "webSourcePath": "dist",
        "streamServiceName": "clickme"
      }
    }
  ]
}